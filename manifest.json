{
	"name": "Your Hybridcast",
    "version": "1.0.0",
    "content_scripts": [
		{
            "matches": ["https://www.youtube.com/watch?*","https://m.youtube.com/watch?*"],
            "js": ["lib/jquery.min.js","main.js"]
        }
    ],
	"options_page": "options.html",
    "icons": {
        "32": "images/icon32.png",
        "48": "images/icon48.png",
		"128": "images/icon128.png"
    },
    "description": "Execute your content on youtube like Hybridcast",
	"manifest_version": 2,
	"permissions": [
        "storage"
    ]
}
